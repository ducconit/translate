{
    "name": "dnt/translate",
    "type": "module",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DNT\\Translate\\": "src/"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "DNT\\Translate\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ducnt",
            "email": "ducnt@sphoton.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.54"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.21"
    },

    "extra": {
        "laravel": {
            "providers": [
                "DNT\\Translate\\TranslateServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }

}
